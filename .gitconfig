[user]
	name = Adrian Toth
[pull]
	rebase = true
[push]
	default = simple
[alias]
	branch-name = "symbolic-ref -q --short HEAD"
	master = "checkout master"
	f = "!git fetch --all --prune && git checkout master && git merge upstream/master --ff-only && git push && echo 'flush_all' | nc localhost 11211"
	fixup = "!HASH=${1-$(git rev-parse HEAD)} && git commit --fixup=$HASH && EDITOR=: git rebase -i --autosquash $HASH~1 && echo"